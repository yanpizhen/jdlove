<% if object.errors.any? %>
  <div class="row">
    <div class='alert alert-danger'>
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
      <ul>
        <% object.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  </div>
<% end %>
