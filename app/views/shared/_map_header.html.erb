<div class="header map-header sr-header own-container-fluid" data-sr-basic="<%= sr_prepare_basic_data('Web') %>">
  <div class="own-container-wrap">
    <!-- 面包屑-->
    <p class="breadcrumbs">
      <a href="/">住百家 > </a>
      <span>地图搜索</span>
    </p>
    <% if user_signed_in? %>
        <div class="in">
          <%= link_to customer_profile_path, class: 'go-profile' do %>
              <span class="user-name sr-user-name" data-sr-user-id="<%= current_user.id %>"><%= current_user.name %></span>
          <% end %>
          <div class="div-wrap">
            <ul class="link-list">
              <li><span class="triangle-up"></span></li>
              <li class="sec-li"><a href="/customer/profile"><span class="home"></span>个人中心</a></li>
              <li><a href="/customer/bookings"><span class="order"></span>我的订单</a></li>
              <li><a href="/customer/favorites"><span class="collection"></span>我的收藏</a></li>
              <li><a href="/customer/wallets"><span class="wallets"></span>我的钱包</a></li>
              <li><a href="/customer/messages"><span class="message"></span>消息中心</a></li>
              <li class="li-hr"><hr></li>
              <li>
                <%= link_to password_customer_profile_path do %>
                    <span class="setting"></span>修改密码
                <% end %>
              </li>
              <li class="sr-out-login">
                <%= link_to destroy_user_session_path do %>
                    <span class="out"></span>退出登录
                <% end %>
              </li>
            </ul>
          </div>
        </div>
    <% else %>
        <div class="out">
          <span class="login sr-header-login" id="signIn">登录</span>
          <span class="register sr-header-register" id="signUp">注册</span>
        </div>
        <script type="text/javascript">
          $('#signIn').on('click', function() { ZBJ.SignSwitcher.showSignIn(); });
          $('#signUp').on('click', function() { ZBJ.SignSwitcher.showSignUp(); });
        </script>
    <% end %>

    <span class="ask-online gtm-header-online sr-header-online" onClick="NTKF.im_openInPageChat()">在线咨询</span>

    <p class="download sr-download">
      <span class="text">手机订房更方便</span>
        <span class="qr">
          <span class="triangle-up"></span>
          <span>扫描二维码下载手机客户端</span>
          <img src="<%= asset_path("web/footer/download_qr_code.jpg") %>" alt="下载二维码" class="qr-img">
          <span>下载即送<span class="down-big">￥50</span>优惠券</span>
        </span>
    </p>
  </div>
</div>
